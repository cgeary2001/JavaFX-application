<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.ViewEventController">
   <children>
      <Button id="btnBack" fx:id="btnBack" layoutX="25.0" layoutY="14.0" mnemonicParsing="false" onAction="#backToTimeline" text="Back To Register" AnchorPane.leftAnchor="25.0" />
      <Button id="btnEdit" layoutX="150.0" layoutY="14.0" mnemonicParsing="false" text="Edit Event" />
      <Label id="lblEvent" fx:id="eventTitleLbl" layoutX="25.0" layoutY="58.0" text="Event 1">
         <font>
            <Font name="Arial Black" size="15.0" />
         </font>
      </Label>
      <Button id="btnNext" fx:id="nextEventBtn" layoutX="497.0" layoutY="14.0" mnemonicParsing="false" onAction="#nextLinkedEvent" prefHeight="25.0" prefWidth="93.0" text="Next Event" AnchorPane.rightAnchor="10.0" />
      <Label id="lblDesc" layoutX="26.0" layoutY="141.0" text="Description" AnchorPane.leftAnchor="25.0" />
      <TextArea fx:id="descArea" editable="false" layoutX="106.0" layoutY="166.0" prefHeight="146.0" prefWidth="547.0" AnchorPane.rightAnchor="25.0" />
      <Label id="lblLoc" layoutX="27.0" layoutY="338.0" text="Location" AnchorPane.leftAnchor="25.0" />
      <Label id="lblAttach" layoutX="328.0" layoutY="338.0" text="Attachments" />
      <TextArea editable="false" layoutX="320.0" layoutY="364.0" prefHeight="97.0" prefWidth="245.0" AnchorPane.rightAnchor="25.0" />
      <TextArea fx:id="locationArea" editable="false" layoutX="28.0" layoutY="364.0" prefHeight="97.0" prefWidth="245.0" />
      <TextArea fx:id="dateArea" editable="false" layoutX="395.0" layoutY="57.0" prefHeight="25.0" prefWidth="196.0" />
      <Button fx:id="prevEventBtn" layoutX="395.0" layoutY="14.0" mnemonicParsing="false" onAction="#prevLinkedEvent" text="Previous Event" />
   </children>
</AnchorPane>
